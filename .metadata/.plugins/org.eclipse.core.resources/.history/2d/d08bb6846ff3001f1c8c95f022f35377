spring.application.name=shop

# Data source configuration
spring.datasource.url=jdbc:mysql://localhost:3306/onlinebookshop
spring.datasource.username=root
spring.datasource.password=Genpact@123456789
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# enable sql script for table creation 
 spring.sql.intit.mode=ALWAYS
 #show sql queries in console
 spring.jpa.show-sql=true
 #sql initialization platform
 spring.sql.intit.platform=mysql
 
